{
  "sourceMaps": true,
  "module": {
    "type": "es6",
    "resolveFully": true
  },
  "jsc": {
    "externalHelpers": false,
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true,
      "importAssertions": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "baseUrl": "./",
    "paths": {
      "@app/*": ["src/*"],
      "@mediaserver/*": ["src/mediaserver/*"]
    }
  }
}

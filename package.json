{
  "name": "api-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Ali Mansoori",
  "license": "Proprietary",
  "private": true,
  "homepage": "https://git.localic.com/backend/api-backend",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@git.localic.com:backend/api-backend.git"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma.seeder.ts"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint:check"
    ]
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm swc",
    "clean": "shx rm -rf ./dist",
    "db:gen": "npx prisma generate",
    "prisma:copy": "shx cp -R node_modules/.pnpm/@prisma+client@5.3.1_prisma@5.3.1/node_modules/.prisma/* node_modules/.prisma/",
    "db:reset": "dotenv -e .env -- npx prisma migrate reset",
    "db:reset:test": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "db:push:test": "dotenv -e .env.test -- npx prisma db push --force-reset --skip-generate",
    "db:make:test": "dotenv -e .env -- npx prisma migrate dev --name test",
    "prisma:merge": "node scripts/mergeSchemas.js",
    "db:make": "npm run prisma:merge && dotenv -e .env -- npx prisma migrate dev",
    "db:make:quick": "shx rm -r prisma/migrations/* & echo y | npm run db:make --name=quick",
    "db:deploy": "npm run prisma:merge && dotenv -e .env -- npx prisma migrate deploy ",
    "db:seed:up": "dotenv -e .env -- npx prisma db seed",
    "db:seed:up:test": "dotenv -e .env.test -- npx prisma db seed",
    "db:backup": "mysqldump -h localhost -u root -p secret > db/backup.sql",
    "schema:gen": "node --loader ./loader.js src/core/scripts/schemaGenerator/index.ts",
    "schema:env:gen": "node --loader ./loader.js src/core/scripts/envSchemaGenerator/index.ts",
    "circular-dependency:check": "npx madge --circular --extensions ts ./src",
    "circular-dependency:graph": "npx madge --circular --extensions ts --image graph.svg ./src",
    "unused:check": "npx ts-unused-exports ./tsconfig.json",
    "test": "dotenv -e .env.test mocha",
    "test:reset": "dotenv -e .env.test cross-env RESET_DATABASE=true mocha",
    "dev": "node --loader ./loader.js src/core/scripts/devBuild/index.ts",
    "start": " cross-env API_BACKEND_PORT=3002 node dist/index.js",
    "stress-test": "node --loader ./loader.js src/utility/stressTest",
    "dev:debug": "node --loader ./loader.js src/core/scripts/devBuild/index.ts --debug",
    "docker:build": "docker build",
    "prepare": "husky config/.husky",
    "lint:check": "eslint \"src/**/*.ts\" --cache",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "env:build": "shx cp .env.example .env",
    "swc": "npx swc --copy-files ./src --out-dir dist --strip-leading-paths",
    "swc:watch": "npx swc -w --copy-files ./src --out-dir dist --strip-leading-paths",
    "swc:config:gen": "ts-node-esm --transpile-only src/core/scripts/tsconfigToSwc/index.ts",
    "mediaserver:dev": "cross-env IS_MEDIASERVER=true node --watch --loader ./loader.js src/mediaserver-startup.ts",
    "mediaserver:start": "cross-env IS_MEDIASERVER=true node dist/mediaserver-startup.js",
    "chroma:seed:up": "dotenv -e .env -- npx node --loader ./loader.js chromadb/seed.ts",
    "newman:run": "newman run postman_collection_api_testing.json -r htmlextra",
    "doc": "typedoc --darkHighlightTheme dark-plus"
  },
